{
    "name": "actengage/deployer",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Actengage\\Deployer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jacob Lockard",
            "email": "jlockard@actengage.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
      "php": "^8.1",
      "laravel/framework": "^9.0|^10.0",
      "laravel/pint": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Actengage\\Deployer\\ServiceProvider"
            ]
        }
    }
}
